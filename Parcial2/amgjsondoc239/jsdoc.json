{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [ "./src"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/better-docs",  
        "encoding": "utf8",               
        "destination": "./out/",          
        "recurse": true,                  
        "tutorials": "./tutorials"
    }
}